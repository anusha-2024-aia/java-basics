public class DoWhileDemo {
    public static void main(String[] args) {
        int i = 1;

        do {
            System.out.println("i = " + i);
            i++; // Increment the counter
        } while (i <= 5); // Condition to continue the loop
    }
}


public class GFG {
  
    public static void main(String[] args) {
      
        int c = 1;

      // Using do-while loop
        do {
            System.out.println("GeeksforGeeks: " + c);
            c++;
        } while (c <= 5);
    }
}